        "@types/react": "^19.1.8",
        "@types/react-dom": "^19.1.6",
        "miniflare": "^4.20250604.1",
        "react-router-devtools": "^5.0.6",
        "tailwindcss": "^4.1.10",
        "typescript": "^5.8.3",
